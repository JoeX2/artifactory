<VirtualHost *:<%= node["artifactory"]["apache"]["port"] %>>
	ServerName <%= node["artifactory"]["apache"]["server_name"] || "artifactory.#{node["domain"]}" %>
	ProxyPass / http://localhost:<%=node["artifactory"]["port"] %>/
	ProxyPassReverse / http://localhost:<%=node["artifactory"]["port"] %>/
	ProxyRequests Off
	ProxyPreserveHost on
	
	<Proxy http://localhost:8080/*>
    	Order deny,allow
    	Allow from all
	</Proxy>

</VirtulHost>